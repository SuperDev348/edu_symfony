                            <div class="place-inner">
                                <div class="place-thumb">
                                    <a class="entry-thumb" href="{{ path('listing_detail', {'id': similar_listing.getId}) }}"><img src="{{ asset(similar_listing.getCoverImage) }}" alt=""></a>
                                    <a href="#" class="golo-add-to-wishlist btn-add-to-wishlist " data-place-id="185">
                                        <span class="icon-heart">
                                            <i class="la la-bookmark large"></i>
                                        </span>
                                    </a>
                                    <a class="entry-category purple" href="#">
                                        <i class="las la-spa"></i><span>{{ similar_listing.category.getName }}</span>
                                    </a>
                                    <a href="#" class="author" title="Author"><img src="{{ asset(similar_listing.user.getImage) }}" alt="Author"></a>
                                </div>       
                                <div class="entry-detail">
                                    <div class="entry-head">
                                        <div class="place-type list-item">
                                            <span>{{ similar_listing.category.getName }}</span>
                                        </div>
                                        <div class="place-city">
                                            <a href="#">{{ similar_listing.city.getName }}</a>
                                        </div>
                                    </div>
                                    <h3 class="place-title"><a href="{{ path('listing_detail', {'id': similar_listing.getId}) }}">{{ similar_listing.getName }}</a></h3>
                                    <div class="open-now"><i class="las la-door-open"></i>Open now</div>
                                    <div class="entry-bottom">
