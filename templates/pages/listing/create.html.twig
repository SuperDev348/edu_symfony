                    <!-- <div id="editor"></div> -->
                </div>
                <div class="field-group field-select">
                    <label for="category">catégorie</label>
                    <select data-placeholder="Veuillez choisir une catégorie" name="category" id="category">
                        <option value="See & Do">See & Do</option>
                        <option value="Food & Drink">Food & Drink</option>
                        <option value="Stay">Stay</option>
                        <option value="Shop">Shop</option>
                    </select>
                    <i class="la la-angle-down"></i>
                </div>
                <div class="field-group field-select">
                    <label for="place_type">Type d'annonce</label>
                    <select data-placeholder="Veuillez choisir un type" name="place_type" id="place_type">
                        <option value="Bar">Bar</option>
                        <option value="Sea Food">Sea Food</option>
                    </select>
                    <i class="la la-angle-down"></i>
                </div>
                <div class="field-group field-select">
                    <label for="city">City</label>
