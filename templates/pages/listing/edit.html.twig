                <li><a href="#open" title="Horaires"><span class="icon"><i class="la la-business-time"></i></span><span>Horaires</span></a></li>
                <li><a href="#media" title="Photos/Videos"><span class="icon"><i class="la la-image"></i></span><span>Photos/Videos</span></a></li>
            </ul>
        </div>
    </div><!-- .listing-nav -->
    <div class="listing-content">
        <h2>Ajouter une annonce</h2>
        <form action="{{ path('listing_update', {'id': listing.getId}) }}" class="upload-form" id="add_listng_from" method="POST" enctype="multipart/form-data">
            <div class="listing-box" id="genaral">
                <h3>Informations générales</h3>
                <div class="field-inline">
                    <div class="field-group field-input">
                        <label for="name">Nom de l'annonce</label>
                        {% if (old is defined) and ('name' in old|keys) %}
                        <input type="text" id="name" placeholder="Le nom de votre annonce" name="name" value="{{ old.name }}">
                        {% else %}
                        <input type="text" id="name" placeholder="Le nom de votre annonce" name="name" value="{{ listing.getName }}">
                        {% endif %}
                        {% if (errors is defined) and ('name' in errors|keys) %}
                        <span style="color: red;">{{ errors.name }}</span>
                        {% endif %}
                    </div>
                    <div class="field-group field-select">
                        <label for="price_range">les prix</label>
                        <select name="price_range" id="price_range">
                            <option value="0">None</option>
                            <option value="1">$</option>
                            <option value="2">$$</option>
                            <option value="3">$$$</option>
                            <option value="4">$$$$</option>
                        </select>
                        <i class="la la-angle-down"></i>
                    </div>
                </div>
                <div class="field-group">
                    <h4 class="label">Description</h4>
                    {% if (old is defined) and ('description' in old|keys) %}
                    <textarea name="description" rows="6" style="width: 100%;">{{ old.description }}</textarea>
                    {% else %}
                    <textarea name="description" rows="6" style="width: 100%;">{{ listing.getDescription }}</textarea>
                    {% endif %}
                    {% if (errors is defined) and ('description' in errors|keys) %}
                    <span style="color: red;">{{ errors.description }}</span>
                    {% endif %}
                    <!-- <div id="editor"></div> -->
                </div>
                <div class="field-group field-select">
                    <label for="category">catégorie</label>
                    <select data-placeholder="Veuillez choisir une catégorie" name="category" id="category" value="{{ listing.getCategory }}">
                        <option value="See & Do">See & Do</option>
                        <option value="Food & Drink">Food & Drink</option>
                        <option value="Stay">Stay</option>
                        <option value="Shop">Shop</option>
                    </select>
                    <i class="la la-angle-down"></i>
                </div>
                <div class="field-group field-select">
                    <label for="place_type">Type d'annonce</label>
                    <select data-placeholder="Veuillez choisir un type" name="place_type" id="place_type" value="{{ listing.getPlaceType }}">
                        <option value="Bar">Bar</option>
                        <option value="Sea Food">Sea Food</option>
                    </select>
                    <i class="la la-angle-down"></i>
                </div>
                <div class="field-group field-select">
                    <label for="city">City</label>
                    <select data-placeholder="" name="city" id="city" value="{{ listing.getCity }}">
                        <option value="paris">Paris</option>
                        <option value="newyork">New York</option>
                        <option value="chicago">Chicago</option>
                    </select>
                    <i class="la la-angle-down"></i>
                </div>
            </div><!-- .listing-box -->
            <div class="listing-box" id="hightlight">
                <h3>Points forts</h3>
                <div class="field-group field-check">
